{
  "env": {
    "es6": true,
    "node": true,
    "browser": true  
   },
  "rules": {
    //"indent": [2, 4],            
    "semi": [2, "always"],       
    "no-unused-vars": 1,         
    "space-unary-ops": [0, {     
      "words": false,             
      "nonwords": false          
    }],           
    "strict": [1, "global"], 
    //"no-console": 1,
    "new-cap":1,
    "no-use-before-define":1,
    "eqeqeq": 2
  },
  "globals": {                   
    "$": true,
    "app": true,
    "module": true,
    "require": true
  }
}